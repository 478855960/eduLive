<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.yiming.dao.UserDao">
	  <select id="login" resultType="com.yiming.entity.User">
          select * from user where phoneNum=#{0} and password=#{1}
      </select>
	  
      <insert id = "register">
          insert into user(phoneNum,password,name,nickname,registerTime,isStudent)
          values(#{phoneNum},#{password},#{name},#{nickname},#{registerTime},#{isStudent})
      </insert>
  </mapper>	
  
  